1: 1(2 # #)(3 4 # # 5 # #)
serialize(root)
  write(root)
  if root is null: return
  serialize(root->left)
  serialize(root->right)
  
write(root)
  if root is null:
    write('#')
  else:
    write(root->val)

deserialize(root)
  read(root)
  if root is null: return null
  root->left = deserialize(root->left)
  root->right = deserialize(root->right)

read(root)
  in >> val
  if val == '#': return null
  return new TreeNode(stoi(val))
