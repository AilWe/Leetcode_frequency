//Transverse to find the minimum number
//T: O(n)
class Solution {
public:
    int findMin(vector<int>& nums) {
        int min = INT_MAX;
        for (int i = 0; i< nums.size(); i++){
            if(nums[i]<min)
                min = nums[i];
        }
        return min;
        
    }
};
//sort then find
//T:O(n*logn)
class Solution {
public:
    int findMin(vector<int>& nums) {
        sort(nums.begin(),nums.end());
        return nums[0];
        
    }
};
